.mr-inner {
    position: relative;
}

    .img .img-responsive,
    .mr-inner .img-responsive {
        width: 100%;
    }

    .mr-inner:after {
        content: "";
        height: 1px;
        background: #fff;
        opacity: 0.5;
        width: 100%;
        position: absolute;
        bottom: 5%;
        left: 0px;
        right: 0px;
    }

    .mr-inner:before {
        content: "";
        height: 90%;
        background-color: rgba(0,0,0,0.6);
        border-top: 1px #ccc solid;
        width: 100%;
        position: absolute;
        top: 5%;
        margin-top: -1px;
        left: 0px;
        right: 0px;
    }

.mr-overlay {
    background: transparent;
    position: absolute;
    bottom: 5%;
    left: 0;
    width: 100%;
    height: 107px;
    padding: 32px 32px 0;
    transition: .4s;
    -moz-transition: .4s;
    -webkit-transition: .4s;
    -ms-transition: .4s;
    cursor: pointer;
}

.mr-inner:hover .mr-overlay {
    background: rgba(65, 84, 48,0.9);
    position: absolute;
    bottom: 5%;
    left: 0;
    width: 100%;
    height: 90%;
    padding: 15px 25px;
    transition: .4s;
    -moz-transition: .4s;
    -webkit-transition: .4s;
    -ms-transition: .4s;
    cursor: pointer;
}

.mr-overlay h2 {
    font-size: 24px;
    line-height: 31px;
    color: #fff;
    font-weight: 400;
    transition: .4s;
    -moz-transition: .4s;
    -webkit-transition: .4s;
    -ms-transition: .4s;
    text-transform: uppercase;
    margin: 0px 0 0px;
}

.mr-overlay h3 {
    font-size: 24px;
    line-height: 31px;
    color: #fff;
    font-weight: 400;
    transition: .4s;
    -moz-transition: .4s;
    -webkit-transition: .4s;
    -ms-transition: .4s;
    text-transform: uppercase;
    margin: 0px 0 0px;
}

.mr-head {
    background: #E7E1D6;
    position: absolute;
    top: 10%;
    left: 10%;
    right: 0;
    padding: 50px 18px;
    width: 80%;
    height: 80%;
}

.mr-inner:hover .mr-overlay h2 {
    margin: 5px 0 10px;
}

.mr-inner:hover .mr-overlay h3 {
    margin: 0px 0 10px;
}

.mr-overlay p {
    font-size: 18px !important;
    line-height: 17px;
    color: #ffffff !important;
    display: block;
    padding-right: 15%;
    opacity: 0;
    /* ReSharper disable once CssNotResolved */
    visbility: hidden;
    transition: .4s;
    -moz-transition: .4s;
    -webkit-transition: .4s;
    -ms-transition: .4s;
    text-shadow: none !important;
}

.mr-inner:hover .mr-overlay p {
    opacity: 1;
    /* ReSharper disable once CssNotResolved */
    visbility: visible;
    transition: .4s;
    -moz-transition: .4s;
    -webkit-transition: .4s;
    -ms-transition: .4s;
    transition-delay: .2s;
}

.room-detail {
    font-weight: 400;
    font-size: 11px;
    line-height: 11px;
    color: #FFF;
    position: absolute;
    bottom: 10px;
    left: 25px;
    transition: .4s;
    -moz-transition: .4s;
    -webkit-transition: .4s;
    -ms-transition: .4s;
    opacity: 0;
    /* ReSharper disable once CssNotResolved */
    visbility: hidden;
}

.mr-inner:hover .room-detail {
    opacity: 1;
    /* ReSharper disable once CssNotResolved */
    visbility: visible;
    transition: .4s;
    -moz-transition: .4s;
    -webkit-transition: .4s;
    -ms-transition: .4s;
    transition-delay: .2s;
}

